#: ../simple-image-reducer.py:333
msgid "Add File..."
msgstr "Добавить файл..."

#: ../simple-image-reducer.py:340
msgid "Image Files"
msgstr "Файлы изображений"

#: ../simple-image-reducer.py:350
msgid "All Files"
msgstr "Все файлы"

#: ../simple-image-reducer.py:384
#, python-format
msgid "Append \"-%s\" to the file base name"
msgstr "Добавить \"-%s\" к базовому имени файла"

#: ../simple-image-reducer.py:386
#, python-format
msgid "Save to \"%s\" subdirectory"
msgstr "Сохранить в подкаталог \"%s\""

#: ../simple-image-reducer.py:390
#, python-format
msgid "%(current)d of %(total)d images processed"
msgstr "Обработано %(current)d из %(total)d изображений"

#: ../simple-image-reducer.py:394
#, python-format
msgid "%d images to process"
msgstr "%d изображений для обработки"

#: ../simple-image-reducer.py:437
#, python-format
msgid "Cannot identify image file: %s"
msgstr "Невозможно определить тип изображения: %s"

#: ../simple-image-reducer.py:439
#, python-format
msgid "Unable to open file: %s"
msgstr "Не открывается файл: %s"

#: ../simple-image-reducer.py:469
#, python-format
msgid "Unable to open file for writing: %s"
msgstr "Не открывается файл для записи: %s"

